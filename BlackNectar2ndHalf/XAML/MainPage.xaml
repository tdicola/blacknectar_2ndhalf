<UserControl x:Class="BlackNectar2ndHalf.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:BlackNectar2ndHalf"
    Height="480" Width="640">
    <StackPanel>
        <MediaElement x:Name="media" Width="0" Height="0" AutoPlay="False" Volume="1.0"/>
        <Grid x:Name="LayoutRoot">
            <local:GameControl VerticalAlignment="Top" HorizontalAlignment="Left"/>
        </Grid>
    </StackPanel>
</UserControl>
